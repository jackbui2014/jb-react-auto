import React, {Fragment, Component} from 'react';
import {globalSettings} from '../../config/config.js';
import './Home.scss';
const Header = React.lazy(() => import('../../layouts/'+globalSettings.layout+'/Header/Header'));
const Footer = React.lazy(() => import('../../layouts/'+globalSettings.layout+'/Footer/Footer'));
const Body = React.lazy(() => import('../../layouts/'+globalSettings.layout+'/Body/Body'));
const Home =(props)=>{
	return (
		<Fragment>
			<Header />
			<Body />
			<Footer />	
		</Fragment>
	);
}
export default Home;