export const SET_IS_MOBILE = 'SET_IS_MOBILE';

export const setIsMobile = isMobile => ({
	type: SET_IS_MOBILE,
	isMobile
})
export default function reducer(state = {
isMobile: false,
}, action){
	switch(action.type){
		case SET_IS_MOBILE:
			return {
				...state,
				isMobile: action.isMobile
			}
		default:
		return {
			...state,
			isMobile: action.isMobile
		}
	}
}